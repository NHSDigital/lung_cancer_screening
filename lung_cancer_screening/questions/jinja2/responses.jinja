{% extends 'layout.jinja' %}


{% block content %}
  <div class="nhsuk-grid-row">
    <div class="nhsuk-grid-column-two-thirds">
      <h1 class="nhsuk-heading-l">Responses</h1>
      <ul class="responses">
        {% for response in responses %}
          <li>{{ response.question }} {{ response.value }}</li>
        {% endfor %}
      </ul>
    </div>
  </div>
{% endblock %}
